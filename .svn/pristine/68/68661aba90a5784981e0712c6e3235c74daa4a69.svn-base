package com.custom.VO;

public class CartVO {

	private int cartCnt;
	private String userId;
	private int pSeq;
	private String pName;
	private String savefilename;
	private String pOption;
	private int optionPrice;
	private int optionStock;
	private int optionCnt;
	
	private int deliveryCharge;
	
	public CartVO() {}
	public CartVO(int cartCnt, String userId,int pSeq, String pName, 
			String savefilename, String pOption, int optionPrice, int optionCnt, int optionStock) {
		super();
		this.cartCnt = cartCnt;
		this.userId = userId;
		this.pSeq = pSeq;
		this.pName = pName;
		this.savefilename = savefilename;
		this.pOption = pOption;
		this.optionPrice = optionPrice;
		this.optionCnt = optionCnt;
		this.optionStock = optionStock;
	}
	
	public String getSavefilename() {
		return savefilename;
	}
	public void setSavefilename(String savefilename) {
		this.savefilename = savefilename;
	}
	public int getDeliveryCharge() {
		return deliveryCharge;
	}
	public void setDeliveryCharge(int deliveryCharge) {
		this.deliveryCharge = deliveryCharge;
	}
	public int getOptionStock() {
		return optionStock;
	}
	public void setOptionStock(int optionStock) {
		this.optionStock = optionStock;
	}
	public int getOptionCnt() {
		return optionCnt;
	}
	public void setOptionCnt(int cnt) {
		this.optionCnt = cnt;
	}
	public int getCartCnt() {
		return cartCnt;
	}
	public void setCartCnt(int cartCnt) {
		this.cartCnt = cartCnt;
	}
	public int getOptionPrice() {
		return optionPrice;
	}
	public void setOptionPrice(int optionPrice) {
		this.optionPrice = optionPrice;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public int getpSeq() {
		return pSeq;
	}
	public void setpSeq(int pSeq) {
		this.pSeq = pSeq;
	}
	public String getpName() {
		return pName;
	}
	public void setpName(String pName) {
		this.pName = pName;
	}
	public String getpOption() {
		return pOption;
	}
	public void setpOption(String pOption) {
		this.pOption = pOption;
	}

	
}
